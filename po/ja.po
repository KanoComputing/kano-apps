msgstr "ツール"

#: ..\kano_apps\AppGrid.py:55
msgid "others"
msgstr "その他"

#: ..\kano_apps\AppGrid.py:55
msgid "experimental"
msgstr "実験中"

#: ..\kano_apps\AppGrid.py:247
msgid "Remove from desktop"
msgstr "デスクトップから削除"

#: ..\kano_apps\AppGrid.py:250
msgid "Add to desktop"
msgstr "デスクトップに追加"

#: ..\kano_apps\AppGrid.py:418
msgid "Error"
msgstr "エラー"

#: ..\kano_apps\AppGrid.py:419
msgid "Unable to start the application."
msgstr "アプリを起動できません。"

#: ..\kano_apps\AppGrid.py:421 ..\kano_apps\AppInstaller.py:98
#: ..\kano_apps\AppInstaller.py:177
msgid "OK"
msgstr "了解"

